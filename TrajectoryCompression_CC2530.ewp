<?xml version="1.0" encoding="UTF-8"?>
<project>
  <configuration>
    <name>Debug</name>
    <toolchain>8051</toolchain>
    <debug>true</debug>
    <target>
      <name>CC2530</name>
      <core>8051</core>
      <code_model>small</code_model>
      <data_model>small</data_model>
    </target>
    <compiler>
      <name>icc8051</name>
      <version>9.30.1</version>
      <options>
        <define>DEBUG_MODE</define>
        <define>MAX_ARRAY_SIZE=512</define>
        <define>MAX_TRAJECTORY_POINTS=200</define>
        <optimization>2</optimization>
        <code_model>small</code_model>
        <data_model>small</data_model>
        <core>8051</core>
        <no_unroll>true</no_unroll>
        <no_inline>true</no_inline>
        <include_path>src</include_path>
        <include_path>src/utils</include_path>
        <include_path>src/compressor</include_path>
        <include_path>src/decompressor</include_path>
        <include_path>test/baselines/chimp128</include_path>
      </options>
    </compiler>
    <linker>
      <name>ilink8051</name>
      <version>9.30.1</version>
      <options>
        <code_model>small</code_model>
        <data_model>small</data_model>
        <core>8051</core>
        <stack_size>512</stack_size>
        <heap_size>1024</heap_size>
      </options>
    </linker>
    <debugger>
      <name>cspybat</name>
      <version>9.30.1</version>
      <driver>CC2530</driver>
    </debugger>
  </configuration>
  
  <configuration>
    <name>Release</name>
    <toolchain>8051</toolchain>
    <debug>false</debug>
    <target>
      <name>CC2530</name>
      <core>8051</core>
      <code_model>small</code_model>
      <data_model>small</data_model>
    </target>
    <compiler>
      <name>icc8051</name>
      <version>9.30.1</version>
      <options>
        <define>NDEBUG</define>
        <define>MAX_ARRAY_SIZE=512</define>
        <define>MAX_TRAJECTORY_POINTS=200</define>
        <optimization>2</optimization>
        <code_model>small</code_model>
        <data_model>small</data_model>
        <core>8051</core>
        <no_unroll>true</no_unroll>
        <no_inline>true</no_inline>
        <include_path>src</include_path>
        <include_path>src/utils</include_path>
        <include_path>src/compressor</include_path>
        <include_path>src/decompressor</include_path>
        <include_path>test/baselines/chimp128</include_path>
      </options>
    </compiler>
    <linker>
      <name>ilink8051</name>
      <version>9.30.1</version>
      <options>
        <code_model>small</code_model>
        <data_model>small</data_model>
        <core>8051</core>
        <stack_size>512</stack_size>
        <heap_size>1024</heap_size>
      </options>
    </linker>
  </configuration>
  
